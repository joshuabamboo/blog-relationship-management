<table>
  <tr>
    <th>Title</th>
    <th>Link</th>
    <th>Contact</th>
    <th>Notes</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
 
<% @blogs.each do |blog| %>
  <tr>
    <td><%= blog.title %></td>
    <td><%= blog.link %></td>
    <td><%= blog.contact %></td>
    <td><%= blog.notes %></td>
    <td><%= link_to "Show", blog %></td>
    <td><%= link_to "Edit", edit_blog_path(blog) %></td>
    <td><%= link_to "Delete", blog, method: :delete, data: { confirm: "Are you sure?" } %></td>
  </tr>
<% end %>
</table>